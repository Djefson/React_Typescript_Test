    "fela-plugin-named-keys": "^11.3.3",
    "fela-plugin-prefixer": "^11.3.3",
    "fela-plugin-responsive-value": "^11.3.3",
    "fela-plugin-rtl": "^11.3.3",
    "fela-plugin-theme-value": "^11.4.0",
    "fela-plugin-unit": "^11.3.3",
    "fela-sort-classnames": "^11.3.3",
    "fela-sort-media-query-mobile-first": "^11.3.3",
    "fela-tools": "^11.3.3",
    "hex-to-rgba": "^1.0.2",
    "react-fela": "^11.3.3"
  },
  "peerDependencies": {
    "react": ">= 16.3.0",
    "react-dom": ">= 16.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
