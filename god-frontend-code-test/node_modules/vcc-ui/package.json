{
  "name": "vcc-ui",
  "version": "2.10.1",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "files": [
    "README.md",
    "LICENSE",
    "assets/**",
    "lib/**",
    "es/**",
    "docs"
  ],
  "scripts": {
    "build": "npm run build:es && npm run build:lib && npm run build:docs",
    "build:es": "rm -rf es && babel src -d es --ignore 'src/**/test.js'",
    "build:lib": "rm -rf lib && cross-env BABEL_ENV=commonjs babel src -d lib --ignore 'src/**/test.js'",
    "setup": "npm run build",
    "test": "cd ../../  && jest --config=jest.config.js packages/vcc-ui",
    "watch": "npm run watch:lib & npm run watch:es",
    "watch:lib": "cross-env BABEL_ENV=commonjs babel src -d lib -w",
    "watch:es": "babel src -d es -w",
    "build:docs": "node scripts/build-docs.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@volvo-cars/ui-design-tokens": "^2.10.1",
    "css-in-js-utils": "^3.0.4",
